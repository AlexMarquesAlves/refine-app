{
  "name": "heavy-baths-sort",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 refine dev",
    "build": "refine build",
    "start": "refine start",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "refine": "refine",
    "codegen": "graphql-codegen",
    "commit": "cz",
    "eslint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint src --ext ts,tsx,js,jsx --fix",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx,css,html}\" --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@refinedev/cli": "^2.16.21",
    "@refinedev/core": "^4.47.1",
    "@refinedev/devtools": "^1.1.32",
    "@refinedev/nextjs-router": "^6.0.0",
    "@refinedev/kbar": "^1.3.6",
    "next": "14.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@refinedev/nestjs-query": "^1.1.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.9.1",
    "@refinedev/antd": "^5.37.4",
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.0.5",
    "js-cookie": "^3.0.5",
    "@ant-design/nextjs-registry": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "5.48.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@types/js-cookie": "^3.0.6",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "refine": {
    "projectId": "rgGhZZ-8BdprO-VkRs0a"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint && npm run format"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run eslint",
      "npm run format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}